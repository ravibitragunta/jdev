<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="HomePage">
    <page>/HomePage.jsf</page>
  </view>
  <view id="DetailedPage">
    <page>/DetailedPage.jsf</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>HomePage</from-activity-id>
    <control-flow-case id="__2">
      <from-outcome>gotoDetails</from-outcome>
      <to-activity-id>DetailedPage</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__3">
    <from-activity-id>DetailedPage</from-activity-id>
    <control-flow-case id="__4">
      <from-outcome>gotoHome</from-outcome>
      <to-activity-id>HomePage</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>
